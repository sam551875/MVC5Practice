@model MVC5Practice.Models.Home.ACKB_AOR

@{
    ViewBag.Title = "AorPage1";
}
<script type="text/javascript">

    //$("#F50A").change(function () {
    function F50A_changed() {
        var F50A = $("#F50A").val();
        switch (F50A) {
            case "Option A":
                $("#d_F50_Account").show();
                $("#d_F50_EnglishName").hide();
                $("#d_F50_IdNo").show();
                $("#d_F50_Tel").hide();
                $("#d_F50_Address").hide();
                break;
            case "Option F":
                $("#d_F50_Account").hide();
                $("#d_F50_EnglishName").show();
                $("#d_F50_IdNo").show();
                $("#d_F50_Tel").show();
                $("#d_F50_Address").show();
                break;
            case "Option K":
                $("#d_F50_Account").show();
                $("#d_F50_EnglishName").show();
                $("#d_F50_IdNo").hide();
                $("#d_F50_Tel").hide();
                $("#d_F50_Address").show();
                break;
        }
    }
        
    //});
</script>
